{
  "name": "akari-client",
  "version": "0.1.3",
  "description": "akari grpc client for node.js",
  "type": "commonjs",
  "main": "dist/akari.js",
  "types": "dist/akari.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uboar/akari-client.git"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run clean && tsc -d && mkdir ./dist/akari_proto && cp ./src/akari_proto/*.js ./src/akari_proto/*.d.ts ./dist/akari_proto/",
    "clean": "rm -r dist"
  },
  "author": "uboaruboar",
  "license": "Apache-2.0",
  "dependencies": {
    "@grpc/grpc-js": "^1.8.14",
    "@types/google-protobuf": "^3.15.6",
    "google-protobuf": "^3.21.2",
    "grpc_tools_node_protoc_ts": "^5.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@types/node": "^20.2.5"
  }
}
